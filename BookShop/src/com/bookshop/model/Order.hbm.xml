<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
    <hibernate-mapping package="com.bookshop.model">
    	<class name="Order" table="t_orders">
    		<id name="id" column="id">
    			<generator class="native"></generator>
    		</id>
    		
    		<property name="user_id" column="user_id"></property>
    		<property name="money" column="money"></property>
    		<property name="receiverName" column="receiverName"></property>
    		<property name="receiverPhone" column="receiverPhone"></property>
    		<property name="receiverAddress" column="receiverAddress"></property>
    		<property name="payState" column="payState"></property>
    		<property name="ordertime" column="ordertime"></property>
    		
    	</class>
    </hibernate-mapping>