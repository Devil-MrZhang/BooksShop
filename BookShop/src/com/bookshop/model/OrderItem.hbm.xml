<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
    <hibernate-mapping package="com.bookshop.model">
    	<class name="OrderItem" table="t_orderitem">
    		<id name="id" column="id">
    			<generator class="native"></generator>
    		</id>
    		
    		<property name="order_id" column="order_id"></property>
    		<property name="product_id" column="product_id"></property>
    		<property name="buynum" column="buynum"></property>
    		
    		
    	</class>
    </hibernate-mapping>