<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping package="com.bookshop.model">
    	<class name="Product" table="t_product">
    		<id name="id" column="id">
    			<generator class="native"></generator>
    		</id>
    		
    		<property name="name" column="name"></property>
    		<property name="price" column="price"></property>
    		<property name="img_url" column="img_url"></property>
    		<property name="pnum" column="pnum"></property>
    		<property name="category" column="category"></property>
    		<property name="description" column="description"></property>
    		
    	</class>
    </hibernate-mapping>