<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	
	<package name="admin" namespace="/admin" extends="struts-default">
	
		<action name="AdminAction" class="com.bookshop.action.AdminAction" method="execute">
			<result name="success">/admin/products/list.jsp</result>
		</action>
		
		<action name="CheckAction" class="com.bookshop.action.AdminAction" method="check">
			<result name="check">/admin/products/list.jsp</result>
		</action>
	</package>
	
</struts>
